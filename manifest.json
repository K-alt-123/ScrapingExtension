{
    "manifest_version": 3,
    "name": "My React Chrome Extension",
    "version": "1.0",
    "description": "A Chrome extension built with React",
    "action": {
      "default_popup": "/build/index.html",
      "default_icon": {
        "16": "/build/favicon.ico",
        "48": "/build/favicon.ico",
        "128": "/build/favicon.ico"
      }
    },
    "permissions": [
      "activeTab"
    ],
    "background": {
      "service_worker": "/public/background.js"
    }
  }      